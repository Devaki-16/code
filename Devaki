bWidget = func_verifyObject(objSalesCycle)

If bWidget Then
    rsErrMsg = "Success Sales Cycle Widget is Available"
    Reporter.ReportEvent micPass, "Sales Cycle Widget: Passed", rsErrMsg
    bWidget = True

    If objSalesCycle.GetROProperty("text") = "N" Then
        objSalesCycle.Click
        Wait 5
    Else
        Reporter.ReportEvent micDone, "Sales Cycle Widget: Skipped", "Value is not 'N'"
    End If

Else
    rsErrMsg = "Error Sales Cycle Widget is NOT Available"
    Reporter.ReportEvent micFail, "Sales Cycle Widget: Failed", rsErrMsg
    bWidget = False
End If
