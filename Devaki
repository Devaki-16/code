If objSalesCycle.Exist(5) And objSalesCycle.GetROProperty("enabled") = True Then
    objSalesCycle.WaitProperty "enabled", True, 10000
    objSalesCycle.Highlight
    objSalesCycle.SetFocus
    objSalesCycle.Set "On"
    bWidget = True
    rsErrMsg = "Success Sales Cycle Widget is Available"
    Reporter.ReportEvent micPass, "Sales Cycle Widget: Passed", rsErrMsg
Else
    bWidget = False
    rsErrMsg = "Error Sales Cycle Widget is NOT Available. Exists: " & objSalesCycle.Exist(0) & _
               ", Visible: " & objSalesCycle.GetROProperty("visible") & _
               ", Enabled: " & objSalesCycle.GetROProperty("enabled")
    Reporter.ReportEvent micFail, "Sales Cycle Widget: Failed", rsErrMsg
End If
